@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>

    <!-- Link to the external CSS file -->
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>
    <div class="text-center">
        <h1>Welcome to Somesh's Website</h1>
        <p>To learn more about <a href="https://www.google.com/">click here</a>.</p>
        <button class="btn-custom" onclick="window.location.href='https://www.google.com/'">Learn More</button>
    </div>
</body>
</html>
